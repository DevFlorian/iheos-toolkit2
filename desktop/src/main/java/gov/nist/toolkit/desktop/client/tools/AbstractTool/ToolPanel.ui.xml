<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:f="urn:import:gov.nist.toolkit.desktop.client.widgets">
    <g:SplitLayoutPanel>
        <g:center>
            <g:FlowPanel ui:field="main">
                <!--<g:Label text="main"/>-->
            </g:FlowPanel>
        </g:center>
        <g:east size="150">
            <g:LayoutPanel>
                <g:layer>
                    <f:ContextStackPanel ui:field="contextStack"/>
                </g:layer>
                <g:layer top="50%" height="50%">
                    <f:ButtonStackPanel ui:field="buttonStack"/>
                </g:layer>
            </g:LayoutPanel>
        </g:east>
    </g:SplitLayoutPanel>
</ui:UiBinder>